<!-- The only purpose of this page is to approve records using a custom list button -->
<!-- When called, approves selected records and refreshes the page					-->
<!-- Since it doesn't display anything to the user, it is self-closing				-->
<apex:page standardController="PTO__c" 
           recordSetVar="PTOs" 
           tabStyle="PTO__c"
           extensions="afApproveButtonExtension" 
           action="{!saveAndRefresh}"/>